{
  "name": "bookshelf",
  "version": "1.0.0",
  "description": "Code for QuantEcon's Bookshelf site",
  "private": true,
  "main": "./app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-server": "node app.js",
    "start-client": "cd client && npm start",
    "start:dev": "concurrently \"npm run start-server\" \"npm run start-client\"",
    "start": "node app.js",
    "install-all": "npm install && npm run install-client",
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm build",
    "api-docs": "apidoc -f \".*\\.js$\" -i ./routes -o ./docs",
    "client-docs": "jsdoc -r -d ./client/docs -c ./jsdoc-config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jstac/QuantEconLib.git"
  },
  "author": "@tlyon3",
  "license": "BSD-3",
  "bugs": {
    "url": "https://github.com/jstac/QuantEconLib/issues"
  },
  "homepage": "https://github.com/jstac/QuantEconLib#readme",
  "dependencies": {
    "async": "^2.4.1",
    "body-parser": "^1.18.2",
    "codemirror": "^5.30.0",
    "connect-multiparty": "^2.0.0",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "express-passport-logout": "^0.1.0",
    "express-session": "^1.15.6",
    "fuzzy-time": "^1.0.7",
    "jsonwebtoken": "^8.x.x",
    "mailgun-js": "^0.13.1",
    "mongodb": "^2.2.33",
    "mongoose": "^4.12.1",
    "mongoose-paginate": "^5.0.3",
    "multer": "^1.3.0",
    "multiparty": "^4.1.3",
    "normalize.css": "^7.0.0",
    "passport": "^0.4.x",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth2": "^0.1.6",
    "passport-jwt": "^3.x.x",
    "passport-linkedin-oauth2": "^1.5.0",
    "passport-twitter": "^1.0.4",
    "passport-youtube-v3": "^2.0.0",
    "query-string": "^5.x.x",
    "react-jupyter": "^0.1.0",
    "sprintf": "^0.1.5",
    "tmp": "0.0.33"
  },
  "apidoc": {
    "title": "Bookshelf-Documentation",
    "description": "API documentation for Bookshelf site",
    "url": "http://bookshelf.quantecon.org",
    "order": [
      "Information",
      "Search",
      "Submit",
      "Edit",
      "Delete",
      "Vote",
      "Edit_Profile",
      "Authentication"
    ]
  },
  "devDependencies": {
    "jsdoc-jsx": "^0.1.0"
  }
}
